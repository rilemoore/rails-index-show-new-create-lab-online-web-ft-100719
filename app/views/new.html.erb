<h3>Coupon Form</h3>
<%= form_tag coupons_path do %>
  <label>Coupon first name:</label><br>
  <%= text_field_tag :'student[first_name]' %><br>
 
  <label>Student last name:</label><br>
  <%= text_area_tag :'student[last_name]' %><br>
 
  <%= submit_tag "Submit Student" %>
<% end %>

 
<%= params.inspect %>